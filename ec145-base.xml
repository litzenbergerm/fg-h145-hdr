<?xml version="1.0"?>
<!--
************************************************************************
These are the parameters common to both variants BK117-C2 (EC145) and
BK117-D2 (H145).

************************************************************************
-->

<PropertyList>

  <constants>
      <true type="bool">true</true>
      <false type="bool">false</false>
      <zero type="double">0</zero>
      <one type="double">1</one>
  </constants>
      
  <sim>
    <flight-model>yasim</flight-model>
    <fuel-fraction>0.7</fuel-fraction>
    <tags>
      <tag>Eurocopter</tag>
      <tag>helicopter</tag>
      <tag>2-engine</tag>
      <tag>Airbus Helicopters</tag>
      <tag>FGUK</tag>
      <tag>Rotary Wing</tag>
      <tag>1990s</tag>
      <tag>2000s</tag>
    </tags>


    <flight-recorder include="Systems/flight-recorder.xml"/>

    <multiplay>
      <generic>
	<string n="2" type="string">EC-145</string>
		   <!-- Nav lights on -->
      <int n="0" alias="/controls/switches/nav-lights-switch" />
      <!-- Beacon on -->
      <int n="1" alias="/controls/switches/beacon-switch" />
      <!-- Landing lights on -->
      <int n="2" alias="/controls/switches/landing-lights-switch" />
	    <!-- Taxi light on -->
      <int n="3" alias="/controls/switches/strobe-switch" />
	  
	   <int n="6" alias="/gear/gear/ground-is-solid"/><!-- rotor particles -->
	   <!-- Crashed -->
      <int n="7" alias="/sim/crashed" />
	     <!-- search light on -->
      <int n="8" alias="/controls/switches/search-lights-switch" />
	   <!-- Float Inflation -->
      <int n="9" alias="/controls/gear/floats-inflat" />
     
                  
      <float n="0" alias="/sim/model/bk117/door-positions/rightFrontDoor/position-norm"/>
      <float n="1" alias="/sim/model/bk117/door-positions/leftFrontDoor/position-norm"/>
      <float n="2" alias="/sim/model/bk117/door-positions/rightBackDoor/position-norm"/>
      <float n="3" alias="/sim/model/bk117/door-positions/leftBackDoor/position-norm"/>
      <float n="4" alias="/sim/model/bk117/door-positions/rightRearDoor/position-norm"/>
      <float n="5" alias="/sim/model/bk117/door-positions/leftRearDoor/position-norm"/>
      <float n="6" alias="/orientation/pitch-deg"/>	<!-- rotor particles -->
      <float n="7" alias="/position/gear-agl-m"/>	<!-- rotor particles -->
      <float n="8" alias="/orientation/roll-deg"/><!-- rotor particles -->
      <float n="9" alias="/rotors/main/torque"/>	<!-- rotor particles -->
          
      <float n="10" alias="/sim/model/turret-positions/turretx"/><!--Searchlight over MP -->
      <float n="11" alias="/sim/model/turret-positions/turretz"/><!--searchlight over MP -->
      <float n="12" alias="/sim/model/rescue"/><!--winchman over MP -->
      <float n="13" alias="/sim/model/rescue-lift"/><!--winchman over MP -->
      <float n="14" alias="/controls/special/wiper-deg"/><!--wiper over MP , added in v1.3-->
      <float n="15" alias="/accelerations/pilot-gdamped"/><!--Pilot G over MP -->
      <string n="1" alias="/sim/weight[5]/selected" /><!-- Payload over multiplayer -->

	
      </generic>
  </multiplay>

      <sound>
              <path>Aircraft/ec145/Sounds/sound.xml</path>
      </sound>


      <systems> 
        <electrical include="Systems/electrical.xml"/> 
        
        <autopilot>
          <path>Aircraft/ec145/Systems/autopilotH2.xml</path>
        </autopilot>
        
        <autopilot n="103">
            <path>Aircraft/ec145/Systems/sound-rules.xml</path>
        </autopilot>

        <autopilot n="102">
            <path>Aircraft/ec145/Systems/glass-rain.xml</path>
          </autopilot>
          
        <autopilot n="101">
            <name>Helionix instrumentation drivers</name>
            <path>Aircraft/ec145/Systems/filter.xml</path>      
        </autopilot>

        <autopilot n="104">
            <name>Engine fire and smoke simulation</name>
            <path>Aircraft/ec145/Models/Effects/Engine/fire-and-smoke.xml</path>      
        </autopilot>
        
        <autopilot n="105">
            <path>Aircraft/ec145/Systems/propulsion-rules.xml</path>      
        </autopilot>        
      </systems> 



	<rendering>
    <refl_correction type="string">-0.24</refl_correction>
    </rendering>	
<airport>
        <runways>
            <start-offset-m>3.3</start-offset-m>
        </runways>
</airport>		

		
