    </turbulence>
<precipitation-control>
            <clip-distance type="double">3.75</clip-distance>
        </precipitation-control>
  </environment>
  
  

	<consumables>
                <!-- litzi: needed to change fuel density to keep tank capacity consistent, a bug in fg? -->
		<fuel>
			<tank>
				<name>Main</name>
				<selected type="bool" write="n">1</selected>
				<density-ppg type="double" write="n">6</density-ppg>
			</tank>

			<tank>
				<name>LH Supply</name>
				<selected type="bool" write="n">1</selected>
				<density-ppg type="double" write="n">6</density-ppg>
			</tank>
                        
                        <tank>
                                <name>RH Supply</name>
                                <selected type="bool" write="n">1</selected>
                                <density-ppg type="double" write="n">6</density-ppg>
                        </tank>
		</fuel>
	</consumables>

