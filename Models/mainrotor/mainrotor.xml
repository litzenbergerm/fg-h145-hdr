<?xml version="1.0"?>
<PropertyList>
	<path>hub.gltf</path>
	<offsets>
		<z-m>0.0</z-m>
		<y-m>0</y-m>
		<x-m>0</x-m>
		<pitch-deg>0</pitch-deg>
	</offsets>
	<params>
		<flapA>
			<factor>0.3</factor>
		</flapA>
		<flapB>
			<factor>0.25</factor>
		</flapB>
		<flapC>
			<factor>0.2</factor>
		</flapC>
		<flapD>
			<factor>0.2</factor>
		</flapD>
		<flapE>
			<factor>0.15</factor>
		</flapE>
	</params>
	<!--  blades -->
	<model>
		<name>blade-1</name>
		<path>Aircraft/ec145/Models/mainrotor/blade1.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>
	<model>
		<name>blade-2</name>
		<path>Aircraft/ec145/Models/mainrotor/blade2.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
			<heading-deg>90</heading-deg>
		</offsets>
	</model>
	<model>
		<name>blade-3</name>
		<path>Aircraft/ec145/Models/mainrotor/blade3.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
			<heading-deg>180</heading-deg>
		</offsets>
	</model>
	<model>
		<name>blade-4</name>
		<path>Aircraft/ec145/Models/mainrotor/blade4.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
			<heading-deg>270</heading-deg>
		</offsets>
	</model>
	<!-- changed from spin to rotate animation to avoid
       de-sync'ing of rotor parts rotation
  -->
	<animation>
		<name>rotorcomplete</name>
		<object-name>eye</object-name>
		<object-name>star_hub</object-name>
		<object-name>star_cap1</object-name>
		<object-name>star_cap4</object-name>
		<object-name>star_cap3</object-name>
		<object-name>star_cap2</object-name>
		<object-name>swashplate_cap_up</object-name>
		<object-name>blade-1</object-name>
		<object-name>blade-2</object-name>
		<object-name>blade-3</object-name>
		<object-name>blade-4</object-name>
		<object-name>scissorlink_up</object-name>
		<!--       <object-name>rotoraxis</object-name> -->
	</animation>
	<!-- all parts that tilt with swashplate -->
	<animation>
		<name>swashplate_assembly</name>
		<object-name>swashplate_cap</object-name>
		<object-name>swashplate_rotor</object-name>
		<object-name>pitch_link1</object-name>
		<object-name>pitch_link2</object-name>
		<object-name>pitch_link3</object-name>
		<object-name>pitch_link4</object-name>
		<object-name>scissorlink_low</object-name>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>rotorcomplete</object-name>
		<property>rotors/main/blade/position-deg</property>
		<factor>1.0</factor>
		<offset-deg>0</offset-deg>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	<!-- scissorlink locked-track animation
       to compensate swashplate movements 
   -->
	<animation>
		<type>locked-track</type>
		<object-name>scissorlink_low</object-name>
		<center>
			<x-m>0.136</x-m>
			<y-m>0.0</y-m>
			<z-m>1.615</z-m>
		</center>
		<lock-axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</lock-axis>
		<track-axis>
			<x>1</x>
			<y>0</y>
			<z>1</z>
		</track-axis>
		<target-name>scissorlink_up</target-name>
		<target-center>
			<x-m>0.188</x-m>
			<y-m>0.0</y-m>
			<z-m>1.703</z-m>
		</target-center>
	</animation>
	<animation>
		<type>locked-track</type>
		<object-name>scissorlink_low</object-name>
		<center>
			<x-m>0.136</x-m>
			<y-m>0</y-m>
			<z-m>1.615</z-m>
		</center>
		<lock-axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</lock-axis>
		<track-axis>
			<x>1</x>
			<y>0</y>
			<z>1</z>
		</track-axis>
		<target-name>scissorlink_up</target-name>
		<target-center>
			<x-m>0.188</x-m>
			<y-m>0</y-m>
			<z-m>1.703</z-m>
		</target-center>
	</animation>
	<animation>
		<type>locked-track</type>
		<object-name>scissorlink_up</object-name>
		<center>
			<x-m>0.094</x-m>
			<y-m>0</y-m>
			<z-m>1.703</z-m>
		</center>
		<lock-axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</lock-axis>
		<track-axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</track-axis>
		<target-name>scissorlink_low</target-name>
		<target-center>
			<x-m>0.206</x-m>
			<y-m>0</y-m>
			<z-m>1.685</z-m>
		</target-center>
	</animation>
    
    
    
    
	<!-- swashplate tilt movements axis 1,
        the swashplate movement is 90 deg
        offset to the rotordisc movement!-->
	<animation>
		<type>rotate</type>
		<object-name>swashplate_assembly</object-name>
		<object-name>swashplate</object-name>
		<object-name>mixlever20</object-name>
		<object-name>mixlever_U</object-name>
		<property>/controls/flight/fcs/roll</property>
		<factor>7</factor>
		<offset-deg>0</offset-deg>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>1.536</z-m>
			<!--            <z-m>1.615</z-m> -->
		</center>
	</animation>
	<!-- swash plate tilt movements pitch axis  -->
	<animation>
		<type>rotate</type>
		<object-name>swashplate_assembly</object-name>
		<object-name>swashplate</object-name>
		<object-name>mixlever20</object-name>
		<object-name>mixlever_U</object-name>
		<property>/controls/flight/fcs/pitch</property>
		<factor>7</factor>
		<offset-deg>0</offset-deg>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<!--            <z-m>1.615</z-m> -->
			<z-m>1.536</z-m>
		</center>
	</animation>
    <animation>
		<type>rotate</type>
		<object-name>swashplate_assembly</object-name>
		<property>rotors/main/blade/position-deg</property>
		<factor>1.0</factor>
		<offset-deg>0</offset-deg>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	<!-- pitch link rotation compensation 
         using locked-track animation
         to point the pitch link to the 
         blade pitch horn
    -->
	<animation>
		<type>locked-track</type>
		<object-name>pitch_link1</object-name>
		<center>
			<x-m>0.129</x-m>
			<y-m>-0.129</y-m>
			<z-m>1.605</z-m>
		</center>
		<lock-axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</lock-axis>
		<track-axis>
			<x>0.1</x>
			<y>-0.1</y>
			<z>1</z>
		</track-axis>
		<target-name>blade-1</target-name>
		<target-center>
			<x-m>0.165</x-m>
			<y-m>-0.165</y-m>
			<z-m>1.893</z-m>
		</target-center>
	</animation>
	<animation>
		<type>locked-track</type>
