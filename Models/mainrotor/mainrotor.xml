	</animation>
	<!-- swashplate vertical movements with collective pitch -->
	<animation>
		<type>translate</type>
		<object-name>swashplate_assembly</object-name>
		<object-name>swashplate</object-name>
		<object-name>mixlever20</object-name>
		<object-name>mixlever_U</object-name>
		<property>controls/engines/engine[0]/throttle</property>
		<factor>0.05</factor>
		<offset-m>-0.07</offset-m>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
    
	<animation>
		<type>rotate</type>
		<object-name>mixlever_U</object-name>
		<property>/controls/flight/fcs/pitch</property>
		<factor>-7</factor>
		<offset-deg>0</offset-deg>
		<axis>
			<x>1</x>
			<y>0</y>
