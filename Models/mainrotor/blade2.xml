<?xml version="1.0"?>
<PropertyList>
	<path>blademaster.ac</path>
	<params>
		<flapA>
			<factor>0.3</factor>
		</flapA>
		<flapB>
			<factor>0.25</factor>
		</flapB>
		<flapC>
			<factor>0.2</factor>
		</flapC>
		<flapD>
			<factor>0.2</factor>
		</flapD>
		<flapE>
			<factor>0.15</factor>
		</flapE>
	</params>
	<!-- blade color marking-->
	<animation>
		<object-name>color</object-name>
		<type>material</type>
		<ambient>
			<red>1.0</red>
			<green>0.0</green>
			<blue>0.0</blue>
		</ambient>
	</animation>
	<animation>
		<name>bladecomplete</name>
		<object-name>disc1e</object-name>
		<object-name>disc1d</object-name>
		<object-name>disc1c</object-name>
		<object-name>disc1b</object-name>
		<object-name>disc1a</object-name>
		<object-name>disc1</object-name>
		<object-name>blade1</object-name>
		<object-name>blade1a</object-name>
		<object-name>blade1b</object-name>
		<object-name>blade1c</object-name>
		<object-name>blade1d</object-name>
		<object-name>blade1e</object-name>
		<object-name>pitch_horn1</object-name>
		<object-name>blade_mounting_fork1</object-name>
		<object-name>blade_fitting1</object-name>
		<object-name>absorber1</object-name>
		<object-name>pendulum1</object-name>
		<object-name>color</object-name>
		<object-name>line</object-name>
	</animation>
	<!--  transparency for rotor disc when moving, blades must
	not use this effect, as blade shadows will disappear -->
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>disc1e</object-name>
		<object-name>disc1d</object-name>
		<object-name>disc1c</object-name>
		<object-name>disc1b</object-name>
		<object-name>disc1a</object-name>
		<object-name>disc1</object-name>
	</effect>
	<animation>
		<type>material</type>
		<object-name>blade1</object-name>
		<object-name>blade1a</object-name>
		<object-name>blade1b</object-name>
		<object-name>blade1c</object-name>
		<object-name>blade1d</object-name>
		<object-name>blade1e</object-name>
		<transparency>
			<alpha-prop>rotors/main/rpm</alpha-prop>
			<factor>-0.0045</factor>
			<offset>1</offset>
			<max>1</max>
		</transparency>
	</animation>
	<animation>
		<type>select</type>
		<object-name>color</object-name>
		<object-name>line</object-name>
		<object-name>blade1</object-name>
		<object-name>blade1a</object-name>
		<object-name>blade1b</object-name>
		<object-name>blade1c</object-name>
		<object-name>blade1d</object-name>
		<object-name>blade1e</object-name>
		<condition>
			<less-than>
				<property>rotors/main/rpm</property>
				<value>201</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>disc1e</object-name>
		<object-name>disc1d</object-name>
		<object-name>disc1c</object-name>
		<object-name>disc1b</object-name>
		<object-name>disc1a</object-name>
		<object-name>disc1</object-name>
		<condition>
			<greater-than>
				<property>rotors/main/rpm</property>
				<value>180.999</value>
			</greater-than>
		</condition>
	</animation>
	<!--  Blade 2-->
	<animation>
		<type>rotate</type>
		<object-name>bladecomplete</object-name>
		<property>rotors/main/blade[1]/incidence-deg</property>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>1.814282</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>blade1a</object-name>
		<object-name>blade1b</object-name>
		<object-name>blade1c</object-name>
		<object-name>blade1d</object-name>
		<object-name>blade1e</object-name>
		<object-name>disc1a</object-name>
		<object-name>disc1b</object-name>
		<object-name>disc1c</object-name>
		<object-name>disc1d</object-name>
		<object-name>disc1e</object-name>
		<property>rotors/main/blade[1]/flap-deg</property>
		<factor alias="../../params/flapA/factor"/>
		<center>
			<x-m>1.11027474949900276008</x-m>
			<y-m>0</y-m>
			<z-m>1.83294</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>blade1b</object-name>
		<object-name>blade1c</object-name>
		<object-name>blade1d</object-name>
		<object-name>blade1e</object-name>
		<object-name>disc1b</object-name>
		<object-name>disc1c</object-name>
		<object-name>disc1d</object-name>
		<object-name>disc1e</object-name>
		<property>rotors/main/blade[1]/flap-deg</property>
		<factor alias="../../params/flapB/factor"/>
		<center>
			<x-m>1.99203827655311476008</x-m>
			<y-m>0</y-m>
			<z-m>1.83294</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>blade1c</object-name>
		<object-name>blade1d</object-name>
		<object-name>blade1e</object-name>
		<object-name>disc1c</object-name>
		<object-name>disc1d</object-name>
		<object-name>disc1e</object-name>
		<property>rotors/main/blade[1]/flap-deg</property>
		<factor alias="../../params/flapC/factor"/>
		<center>
			<x-m>2.87380180360722676008</x-m>
			<y-m>0</y-m>
			<z-m>1.83294</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>blade1d</object-name>
		<object-name>blade1e</object-name>
		<object-name>disc1d</object-name>
		<object-name>disc1e</object-name>
		<property>rotors/main/blade[1]/flap-deg</property>
		<factor alias="../../params/flapD/factor"/>
		<center>
			<x-m>3.64534488977957476008</x-m>
			<y-m>0</y-m>
			<z-m>1.83294</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>blade1e</object-name>
		<object-name>disc1e</object-name>
		<property>rotors/main/blade[1]/flap-deg</property>
		<factor alias="../../params/flapE/factor"/>
		<center>
			<x-m>4.41688687374751394244</x-m>
			<y-m>0</y-m>
			<z-m>1.83294</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>pendulum1</object-name>
		<property>sim/model/bk117/absorber-angle-deg</property>
		<center>
			<x-m>0.820</x-m>
			<y-m>0</y-m>
			<z-m>1.848</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>
</PropertyList>
