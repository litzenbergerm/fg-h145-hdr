<?xml version="1.0"?>

<!--EC 145 Helicopter animation set -->

<PropertyList>

  <path>ec145.gltf</path>
  
  <params>
    <flapA>
            <factor>0.3</factor>
    </flapA>
    <flapB>
            <factor>0.25</factor>
    </flapB>
    <flapC>
            <factor>0.2</factor>
    </flapC>
    <flapD>
            <factor>0.2</factor>
    </flapD>
    <flapE>
            <factor>0.15</factor>
    </flapE>
  </params>
  
  <!-- include models , sticks -->

   <model>
      <path>Aircraft/ec145/Models/cyclic.xml</path>
      <offsets>
	  <x-m>-2.462</x-m>
	  <y-m> 0.407</y-m>
	  <z-m> -0.26</z-m>
          <pitch-deg>0</pitch-deg>
      </offsets>
  </model>
  
    <model>
      <path>Aircraft/ec145/Models/cyclic.xml</path>
      <offsets>
	  <x-m>-2.465</x-m>
	  <y-m> -0.394</y-m>
	  <z-m> -0.26</z-m>
          <pitch-deg>0</pitch-deg>
      </offsets>
  </model>
  
   <model>
      <name>wiper</name>
      <path>Aircraft/ec145/Models/Divers/wiper/wiper.xml</path>
      <offsets>
	  <x-m>-3.3</x-m>
	  <y-m>-0.01</y-m>
	  <z-m>0.18</z-m>
          <pitch-deg>3</pitch-deg>
      </offsets>
  </model>

  <!-- headset hanging on setset holder-->
 
  <model>
    <name>headset</name>
    <path>Aircraft/ec145/Models/headset/headset.xml</path>
    <offsets>
      <x-m>-2.33</x-m>
      <y-m>0.06</y-m>
      <z-m>0.855</z-m>
    </offsets>
  </model> 
  
  <!-- model animations -->
    
  <animation>
    <type>select</type>
    <object-name>FixedGlass</object-name>
    <object-name>WindowDBL</object-name>
    <object-name>WindowDFL</object-name>
    <object-name>WindowDRL</object-name>
    <object-name>WindowDRR</object-name>
    <object-name>WindowDBR</object-name>
    <object-name>WindowDFR</object-name>    
    <condition>
      <not>
        <equals>
          <property>sim/current-view/internal</property>
          <value>true</value>
        </equals>
      </not>
    </condition>
 </animation>
 
 <animation>
  <type>select</type>
  <object-name>FixedGlassin</object-name>
  <object-name>WindowDBLin</object-name>
  <object-name>WindowDFLin</object-name>
  <object-name>WindowDRLin</object-name>
  <object-name>WindowDRRin</object-name>
  <object-name>WindowDBRin</object-name>
  <object-name>WindowDFRin</object-name>
  <condition>
    <equals>
      <property>sim/current-view/internal</property>
      <value>true</value>
    </equals>
  </condition>
 </animation>

 <!--Doors-->
  
