  <object-name>ReardoorL</object-name>
  <object-name>WindowDRL</object-name>
  <object-name>WindowDRLin</object-name>
  <object-name>intReardoorL</object-name>
  <property>sim/multiplay/generic/float[5]</property>
  <min-deg>-100</min-deg>
  <factor>-100</factor>
  <axis>
   <x1-m>0.97931</x1-m>
   <y1-m>-0.82740</y1-m>
   <z1-m>-0.14812</z1-m>
   <x2-m>1.25218</x2-m>
   <y2-m>-0.79455</y2-m>
   <z2-m>0.22254</z2-m>
  </axis>
 </animation>
 
 <!-- LH rear door strut lock-track animation parts 1..3-->
 
 <animation>
    <type>locked-track</type>
    <object-name>ReardoorStrutL1</object-name>
    <object-name>ReardoorStrutL2</object-name>
    <object-name>ReardoorStrutL3</object-name>
    <center>
        <x-m>1.732</x-m>
        <y-m>-0.595</y-m>
        <z-m>0.466</z-m>
    </center>
    <lock-axis>
       <x>0</x>
       <y>0</y>
       <z>1</z>
    </lock-axis>    
    <track-axis>
       <x>1</x>
       <y>0</y>
       <z>0</z>
    </track-axis>
    <target-name>ReardoorL</target-name>
    <target-center>
        <x-m>2.021</x-m>
        <y-m>-0.450</y-m>
        <z-m>0.466</z-m>
    </target-center>
 </animation>
 
 <animation>
    <type>locked-track</type>
    <object-name>ReardoorStrutL1</object-name>
    <object-name>ReardoorStrutL2</object-name>
    <object-name>ReardoorStrutL3</object-name>
    <center>
        <x-m>1.732</x-m>
        <y-m>-0.595</y-m>
        <z-m>0.466</z-m>
   </center>
    <lock-axis>
       <x>0</x>
       <y>1</y>
       <z>0</z>
    </lock-axis>    
    <track-axis>
       <x>1</x>
       <y>0</y>
       <z>0</z>
    </track-axis>
    <target-name>ReardoorL</target-name>
    <target-center>
        <x-m>2.021</x-m>
        <y-m>-0.450</y-m>
        <z-m>0.466</z-m>
    </target-center>
 </animation>
 
 <!-- LH read door strut animation part 2..3, attached to door, is sim. by translation-->

 <animation>
      <type>translate</type>
      <object-name>ReardoorStrutL2</object-name>
      <object-name>ReardoorStrutL3</object-name>
      <property>sim/multiplay/generic/float[5]</property>
      <factor>0.30</factor>
      <axis>
           <x>1</x>
           <y>0</y>
           <z>0</z>
      </axis>    
 </animation> 
 
 <!--Rotorbrake Lever-->

 <animation>
      <type>slider</type>
      <object-name>RotorBrakeLever</object-name>
      <visible>true</visible>
      <drag-direction>horizontal</drag-direction>
      
      <action>
        <binding>
          <command>property-adjust</command>
          <property>/controls/rotor/brake</property>
          <factor>-0.05</factor>
          <min>0</min>
          <max>1</max>
          <wrap>0</wrap>
        </binding>
      </action>
      
     <hovered>
        <binding>
          <command>set-tooltip</command>
          <label>RotorBrake: %3d%%</label>
          <tooltip-id>rotorbrake</tooltip-id>
          <mapping>percent</mapping>
          <property>/controls/rotor/brake</property>
        </binding>
      </hovered>
 </animation>

 <animation>
    <type>rotate</type>
    <object-name>RotorBrakeLever</object-name>
    <property>/controls/rotor/brake</property>
    <factor>45</factor>
    <center>
      <x-m>-2.005</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.355</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
 </animation> 

 <!--headset holder on center spar-->
 
 <animation>
    <type>rotate</type>
    <object-name>headsetholder</object-name>
    <property>sim/model/bk117/headsetholder/position-deg</property>
    <factor>1</factor>
    <center>
            <x-m>-2.335</x-m>
            <y-m>-0.005</y-m>
            <z-m> 0.873</z-m>
    </center>
    <axis>
            <x>-0.011391</x>
            <y>0</y>
            <z>0.021732</z>
    </axis>
 </animation>

 <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>headsetholder</object-name>
    <condition>
      <equals>
          <property>sim/model/bk117/headsetholder/position-deg</property>
          <value>0</value>
      </equals>
    </condition>
        
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
          <command>property-interpolate</command>
          <property>sim/model/bk117/headsetholder/position-deg</property>
          <value>90</value>
          <rate>90</rate>
      </binding>     
    </action>
 </animation>
        
 <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>headsetholder</object-name>
    
    <condition>
      <equals>
          <property>sim/model/bk117/headsetholder/position-deg</property>
          <value>90</value>
      </equals>
    </condition>
      
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
          <command>property-interpolate</command>
          <property>sim/model/bk117/headsetholder/position-deg</property>
          <value>0</value>
          <rate>90</rate>
      </binding>     
    </action>
 </animation>

  <animation>
    <type>select</type>
    <object-name>headset</object-name>
    <condition>
        <equals>
            <property>sim/model/bk117/headsetholder/position-deg</property>
            <value>90</value>
        </equals>
    </condition> 
  </animation>

  <!-- rotor head cap rotation-->
  
  <animation>
		<type>spin</type>
    	<object-name>rotor</object-name>
		<property>rotors/main/rpm</property>
		<factor>-1.0</factor>
		<axis>
          <object-name>axis.MainRotor</object-name>
		</axis>
  </animation>
    
  <!-- pedals animation -->

  <animation>
    <type>rotate</type>
    <object-name>pedalCoL</object-name>
    <object-name>pedalPltL</object-name>
    <property>controls/flight/rudder</property>
    <min-deg>-20</min-deg>
    <max-deg>20</max-deg>
    <factor>20</factor>
    <center>
        <x-m>-3.0166</x-m>
        <y-m>0</y-m>
        <z-m>-0.2338</z-m>
    </center>
    <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pedalCoR</object-name>
    <object-name>pedalPltR</object-name>
    <property>controls/flight/rudder</property>
    <min-deg>-20</min-deg>
    <max-deg>20</max-deg>
    <factor>-20</factor>
    <center>
        <x-m>-3.0166</x-m>
        <y-m>0</y-m>
        <z-m>-0.2338</z-m>
    </center>
    <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
    </axis>
  </animation> 
  
</PropertyList>
