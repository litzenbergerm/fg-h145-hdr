 <menubar>
   <default>
    <menu n="100">
     <label>EC-145</label>
     <item>
      <label>Select MP-pilot</label>
      <binding>
       <command>nasal</command>
       <script>
        dual_control_tools.
          copilot_dialog.show(aircraft_dual_control.pilot_type);
       </script>
